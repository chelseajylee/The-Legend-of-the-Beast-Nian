const int buttonPin = 2;  
const int ledPin = 13;
int buttonState = 0; 

void setup() {
  pinMode(ledPin, OUTPUT);
  pinMode(buttonPin, INPUT);
}

void loop() {
  buttonState = digitalRead(buttonPin);
  if (buttonState == HIGH) {
    digitalWrite(ledPin1, HIGH);

  } else {
    digitalWrite(ledPin1, LOW);
}
}

 
